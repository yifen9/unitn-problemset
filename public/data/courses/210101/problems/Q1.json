{
  "id": "Q1",
  "title": "Set Operations",
  "date": "2025-01-10",
  "type": "single",
  "question_md": "Let $A=\\\\{1,2\\\\}$ and $B=\\\\{2,3\\\\}$. Which statements are true?",
  "choices": [
    {
      "id": "A",
      "text_md": "$A\\cup B=\\{1,2,3\\}$ if $A=\\{1,2\\}, B=\\{2,3\\}$"
    },
    {
      "id": "B",
      "text_md": "$A\\cap B=\\{1,3\\}$"
    },
    {
      "id": "C",
      "text_md": "$A\\setminus B=\\{2\\}$"
    },
    {
      "id": "D",
      "text_md": "$B\\setminus A=\\{3\\}$"
    }
  ],
  "answer": [
    "A",
    "D"
  ],
  "explanation_md": "$A\\\\cup B=\\\\{1,2,3\\\\}$；$A\\\\cap B=\\\\{2\\\\}$；$A\\\\setminus B=\\\\{1\\\\}$；$B\\\\setminus A=\\\\{3\\\\}$."
}